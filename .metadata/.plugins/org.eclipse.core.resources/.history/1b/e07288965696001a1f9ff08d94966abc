package Assignment15;

public class Assignment15 {

	public static void main(String[] args) {
		
		//declarations
		
		//welcome
		
		//greatest common divisor
		
		//goodbye

	}
	public static int counter ; 
    public static int gcd(int m, int n) {
    	//increment counter
    	  counter++ ;
    	//base cases and recursive call
    	if(n == 0)
    		return m;
    	else if(m == 0)
    		return n;
    	else if(m%n == 0)
    		return n;
    	else
    		return gcd(n,m%n);
    }
}
